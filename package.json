{
	"name": "util.settings",
	"version": "0.0.1",
	"author": "jmquigley <jmquigley@outlook.com>",
	"description": "Settings management singleton for a web application.",
	"private": false,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jmquigley/util.settings"
	},
	"bugs": {
		"url": "https://github.com/jmquigley/util.settings/issues"
	},
	"engines": {
		"node": ">=7.x"
	},
	"main": "./index.umd.min.js",
	"module": "./index.es.min.js",
	"types": "./index.d.ts",
	"files": [
		"index.js",
		"index.*.min.js",
		"index.d.ts"
	],
	"dependencies": {
		"lodash": "^4.17.11"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.3.4",
		"@babel/plugin-transform-runtime": "^7.3.4",
		"@babel/preset-env": "^7.3.4",
		"@babel/preset-typescript": "^7.3.3",
		"@babel/runtime": "^7.3.4",
		"@types/fs-extra": "5.0.5",
		"@types/jest": "^24.0.11",
		"@types/lodash": "^4.14.123",
		"@types/node": "^11.11.3",
		"@types/uuid": "^3.4.4",
		"@types/yargs": "^12.0.9",
		"babel-loader": "^8.0.5",
		"babel-minify": "^0.5.0",
		"babel-preset-minify": "^0.5.0",
		"coveralls": "^3.0.3",
		"enum-values": "^1.2.1",
		"express": "^4.16.4",
		"fs-extra": "^7.0.1",
		"jest": "^24.5.0",
		"localforage": "^1.7.3",
		"on-change": "^1.1.0",
		"prettier": "^1.16.4",
		"puppeteer": "^1.13.0",
		"tslint": "^5.14.0",
		"tslint-config-prettier": "^1.18.0",
		"typescript": "^3.3.3333",
		"util.fixture": "0.0.72",
		"util.join": "^0.0.25",
		"util.log": "^0.0.43",
		"util.pkgwrap": "^0.0.105",
		"util.string": "^0.0.23",
		"util.toolbox": "^0.0.74",
		"webpack": "^4.29.6",
		"webpack-cli": "^3.3.0"
	},
	"pkgwrap": {
		"cleanup": [
			"docs",
			"server.d.ts",
			"server.js.map",
			"server.js"
		]
	},
	"scripts": {
		"all": "yarn install && NODE_ENV=production yarn build && yarn docs && yarn test && yarn manifest",
		"build": "yarn clean && yarn lint && yarn prettier && pkgwrap --build --webpack && yarn build:es",
		"build:es": "minify index.js --out-file index.es.min.js",
		"clean": "pkgwrap --clean",
		"clean:dist": "yarn clean && rimraf node_modules/ yarn.lock",
		"coverage": "pkgwrap --coverage --jest",
		"docs": "pkgwrap --docs",
		"lint": "pkgwrap --lint",
		"manifest": "npm pack --dry-run",
		"posttest": "npm stop",
		"pretest": "npm start",
		"prettier": "prettier --write \"**/*.{ts,tsx,js,json,yaml,yml,css}\"",
		"start": "[ ! `pgrep node-http-test` ] && node server.js setting-test &",
		"stop": "pkill --signal SIGINT node-http-test",
		"test": "jest --runInBand"
	}
}
